{
  "name": "clicker",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "scripts": {
    "prestart": "rm -rf build",
    "start": "razzle start",
    "prebuild": "rm -rf build",
    "build": "razzle build",
    "test": "razzle test --env=jsdom",
    "start:prod": "NODE_ENV=production node build/server.js",
    "lint": "eslint \"src/**\""
